<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.yao.feicui.newsapp">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/defaultpic"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.ActivityLead">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.ActivityLogo">
        </activity>
        <activity android:name=".ui.ActivityMain">
        </activity>
        <activity android:name=".ui.ActivityShow">
        </activity>
        <activity android:name=".ui.ActivityComment">
        </activity>
    </application>

</manifest>